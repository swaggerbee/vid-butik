@using VBRepo
@model List<Medarbejder>
@{
    ViewBag.Title = "AdmMedarbejder";
}

<h2>Opret nyt produkt</h2>
<a href="/Admin/AdmOpredMed/">opret</a>

<h2>Slet eller rediger produkt</h2>
@foreach (var t in Model)
{
    @t.IMG <br />
    @t.Navn<br /> 
    @t.Tekst<br />
    <a href="/Admin/AdmMedarbejderSlet/@t.ID">slet</a><a>Rediger</a><br /><hr />
}


