@using VBRepo
@model VBRepo.Om_OsVM
@{
    ViewBag.Title = "Om os";
}

<div class="container OmOs_Info">
    <div class="five columns">
        <img src="~/Content/IMG/@Model.Omos.IMG" class="u-max-full-width" />
    </div>
    <div class="seven columns"><p>her kommer der en hel masse tekst om hvordan virksomheden operere og alt sådan noget</p></div>
</div>
<section class="container Ansate">


    @foreach (Medarbejder m in Model.Medarbejder)
    {
        <div class="Medarbejder">
            <img src="~/Content/IMG/@m.IMG" class="u-full-width" />
            <h4>@m.Navn</h4>
            <p>@m.Tekst</p>

        </div>
    }
</section>

